   "source": [
    "\n",
    "def divide(arr , low , high):\n",
    "\n",
    "    if low == high:\n",
    "        return arr[low]\n",
    "\n",
    "    mid = (low+high)//2\n",
    "\n",
    "    left = divide (arr , low , mid)\n",
    "    right = divide (arr , mid+1 , high)\n",
    "\n",
    "    return (max(left ,right))"
   ]
