    "def maxDepth(root):\n",
    "        def traversal(root):\n",
    "\n",
    "            if not root:\n",
    "                return 0\n",
    "            left = traversal(root.left)\n",
    "            right = traversal(root.right)\n",
    "\n",
    "            return max(left , right)+1\n",
    "        \n",
    "        return traversal(root)\n"
   ]
