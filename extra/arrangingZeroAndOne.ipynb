    "def minimumSteps(s):\n",
    "       \n",
    "\n",
    "        counter = 0\n",
    "        res = 0\n",
    "\n",
    "        for i in s:\n",
    "            if i==\"1\":\n",
    "                  counter+=1\n",
    "            else:\n",
    "                  res += counter\n",
    "        return res"
