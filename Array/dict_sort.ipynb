   "source": [
    "def sorting(arr):\n",
    "\n",
    "    temp = sorted(arr)\n",
    "    print(temp)\n",
    "\n",
    "    rank = {value : key  for key , value in enumerate(temp)}\n",
    "\n",
    "    return [rank[i] for i in arr]"
   ]
