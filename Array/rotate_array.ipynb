   "source": [
    "def rotate(arr, k):\n",
    "    \n",
    "    n = len(arr)\n",
    "    k%=n\n",
    "    temp = arr[n-k : ]\n",
    "\n",
    "    for i in range(n-k-1 , -1 , -1):\n",
    "        arr[i+k] = arr[i]\n",
    "\n",
    "        \n",
    "    for i in range(k):\n",
    "        arr[i] = temp[i]\n",
    "\n",
    "    return arr\n",
    "\n"
   ]
  },
